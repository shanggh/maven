      <id>shanggh-nexus</id>
      <name>shanggh-nexus</name>
      <url>http://103.41.52.241:8081/nexus/content/repositories/shanggh</url>
    </repository>
    <snapshotRepository>
      <id>shanggh-nexus-snapshots</id>
      <name>shanggh nexus snapshot repository</name>
      <url>http://103.41.52.241:8081/nexus/content/repositories/snapshots</url>
    </snapshotRepository-->
      <repository>
          <id>shanggh-github-snapshot</id>
          <name>shanggh github maven repository snapshot</name>
          <url>file:/db/m2/github</url>
      </repository>
  </distributionManagement> 
  <modules>
    <module>shanggh-dpl</module>
    <module>shanggh-crypto</module>
    <module>shanggh-tools</module>
  </modules>
</project>